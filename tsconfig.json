{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "declaration": true,
    "outDir": "functions/built",
    "esModuleInterop": true,
    "isolatedModules": false,
    "strict": true,
    "allowJs": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "functions/built"
  ]
}
